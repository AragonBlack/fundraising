{
  "name": "@ablack/fundraising-controller-aragon-fundraising",
  "version": "0.1.0",
  "author": "Aragon Black <contact@aragon.black>",
  "contributors": [
    "Olivier Sarrouy <olivier@pando.network>",
    "Cory Dickson <cory@aragon.black>",
    "Deam Hansen <deam@aragon.blackk>"
  ],
  "repository": "github:AragonBlack/fundraising",
  "homepage": "https://github.com/AragonBlack/fundraising",
  "bugs": "https://github.com/AragonBlack/fundraising/issues",
  "license": "(GPL-3.0-or-later OR MIT)",
  "scripts": {
    "build": "cd app && npm run build",
    "install:frontend": "cd app && npm install",
    "lint:contracts": "solium --dir ./contracts",
    "lint:tests": "eslint ./test",
    "lint": "npm run lint:contracts && npm run lint:tests",
    "compile": "truffle compile",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test",
    "test:gas": "GAS_REPORTER=true npm test",
    "test:coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "ganache-cli:test": "./node_modules/@aragon/test-helpers/ganache-cli.sh",
    "publish:major:devchain": "aragon apm publish major --files ./build --environment default",
    "publish": "npm run publish:major:devchain",
    "abi:extract": "truffle-extract --output app/src/abi/ --keys abi",
    "prepublishOnly": "truffle compile --all && npm run abi:extract -- --no-compile"
  },
  "files": [
    "/arapp.json",
    "/build",
    "/contracts",
    "/test"
  ],
  "dependencies": {
    "@ablack/fundraising-interface-core": "^0.1.0",
    "@ablack/fundraising-market-maker-bancor": "^0.1.0",
    "@ablack/fundraising-module-pool": "^0.1.0",
    "@ablack/fundraising-module-tap": "^0.1.0",
    "@aragon/os": "4.2.0"
  },
  "devDependencies": {
    "@ablack/eslint-config-tests": "^1.0.0",
    "@aragon/apps-agent": "1.1.1",
    "@aragon/apps-shared-migrations": "^1.0.0",
    "@aragon/apps-shared-minime": "^1.0.1",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/apps-vault": "4.1.0",
    "@aragon/test-helpers": "^1.1.0",
    "@ablack/fundraising-formula-bancor": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ganache-cli": "^6.0.3",
    "js-sha3": "^0.8.0",
    "prettier": "^1.8.2",
    "solium": "^1.2.3",
    "truffle": "4.1.14",
    "truffle-extract": "^1.2.1"
  }
}

  Dark Turquoise: hsl(187, 100%, 45%);
  Dark Opal: hsl(176, 100%, 43%)
  Opal: hsl(176, 100%, 47%)
Gold:
  Brandy: hsl(40, 52%, 70%)
  Beige: hsl(43, 100%, 96%)
Red:
  Salmon Red: hsl(0, 94%, 73%)
Green:
  Spring Green: hsl(157, 73%, 48%)
Black:
  Black: hsl(0, 0%, 0%)
White:
  White: hsl(0, 0%, 100%)
Aragon Brand:
  oct/
    view: Fan
    colorsPerRow: 5
  Primary: =Purple.Indigo
  Secondary: =Sea.Turquoise
  Black Ash: =Grey.Black Ash
